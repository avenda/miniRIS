-- Name: fn_ciudad_insert(character varying); Type: FUNCTION; Schema: miniris; Owner: grp_ris
--

CREATE FUNCTION fn_ciudad_insert(character varying) RETURNS smallint
    LANGUAGE plpgsql
    AS $_$DECLARE
  descripcion ALIAS FOR $1;
BEGIN
  INSERT INTO miniris.ciudades (ciu_desc) VALUES(descripcion);
  RETURN CURRVAL('miniris.ciudades_ciu_id_seq');
END;$_$;


ALTER FUNCTION miniris.fn_ciudad_insert(character varying) OWNER TO grp_ris;

