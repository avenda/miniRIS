-- Name: fn_ciudad_update(smallint, character varying); Type: FUNCTION; Schema: miniris; Owner: grp_ris
--

CREATE FUNCTION fn_ciudad_update("p_CIU_ID" smallint, "p_CIU_DESC" character varying) RETURNS void
    LANGUAGE plpgsql
    AS $_$--DECLARE
  --id ALIAS FOR $1;
  --descripcion ALIAS FOR $2;
BEGIN
  UPDATE miniris.ciudades SET ciu_desc=p_CIU_DESC WHERE ciu_id=p_CIU_ID;
END;$_$;


